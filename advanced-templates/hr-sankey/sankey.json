            "x1",
            "x2"
          ]
        },
        {
          "type": "formula",
          "as": "type",
          "expr": "'1 - Departments_Previous'"
        },
        {
          "type": "formula",
          "as": "index",
          "expr": "1"
        },
        {
          "type": "formula",
          "as": "org",
          "expr": "datum['__2__']"
        },
        {
          "type": "extent",
          "field": "x2",
          "signal": "extPrev"
        }
      ]
    },
    {
      "name": "data-structure-t-previous",
      "source": "data-structure-base-previous",
      "transform": [
        {
          "type": "formula",
          "as": "sort",
          "expr": "(split(datum['__2__'],' ')[1])"
        },
        {
          "type": "stack",
          "field": "count",
          "groupby": [
            "type"
          ],
          "sort": {
            "field": [
              "__2__",
              "sort"
            ]
          },
          "as": [
            "x1",
            "x2"
          ]
        },
        {
          "type": "formula",
          "as": "type",
          "expr": "'2 - Teams_Previous'"
        },
        {
          "type": "formula",
          "as": "index",
          "expr": "2"
        },
        {
          "type": "formula",
          "as": "org",
          "expr": "datum['__2__']"
        }
      ]
    },
    {
      "name": "data-structure-d-current",
      "source": "data-structure-base-current",
      "transform": [
        {
          "type": "aggregate",
          "groupby": [
            "__3__"
          ],
          "fields": [
            "count"
          ],
          "ops": [
            "sum"
          ],
          "as": [
            "count"
          ]
        },
        {
          "type": "stack",
          "field": "count",
          "groupby": [
            "type"
          ],
          "sort": {
            "field": [
              "__3__"
            ]
          },
          "as": [
            "x1",
            "x2"
          ]
        },
        {
          "type": "formula",
          "as": "type",
          "expr": "'3 - Departments_Current'"
        },
        {
          "type": "formula",
          "as": "index",
          "expr": "5"
        },
        {
          "type": "formula",
          "as": "org",
          "expr": "datum['__3__']"
        },
        {
          "type": "extent",
          "field": "x2",
          "signal": "extCurr"
        }
      ]
    },
    {
      "name": "data-structure-t-current",
      "source": "data-structure-base-current",
      "transform": [
        {
          "type": "formula",
          "as": "sort",
          "expr": "(split(datum['__3__'],' ')[1])"
        },
        {
          "type": "stack",
          "field": "count",
          "groupby": [
            "type"
          ],
          "sort": {
            "field": [
              "__3__",
              "sort"
            ]
          },
          "as": [
            "x1",
            "x2"
          ]
        },
        {
          "type": "formula",
          "as": "type",
          "expr": "'4 - Teams_Current'"
        },
        {
          "type": "formula",
          "as": "index",
          "expr": "4"
        },
        {
          "type": "formula",
          "as": "org",
          "expr": "datum['__3__']"
        }
      ]
    },
    {
      "name": "data-department-previous-size",
      "source": "data-aggregated",
      "transform": [
        {
          "type": "aggregate",
          "groupby": [
            "__0__",
            "__2__"
          ],
          "fields": [
            "count"
          ],
          "ops": [
            "sum"
          ],
          "as": [
            "size"
          ]
        },
        {
          "type": "formula",
          "as": "key",
          "expr": "datum['__0__']+'|'+datum['__2__']"
        }
      ]
    },
    {
      "name": "data-major",
      "source": "data-aggregated",
      "transform": [
        {
          "type": "formula",
          "as": "key",
          "expr": "datum['__0__']+'|'+datum['__2__']"
        },
        {
          "type": "lookup",
          "from": "data-department-previous-size",
          "key": "key",
          "fields": [
            "key"
          ],
          "values": [
            "size"
          ],
          "as": [
            "department_size"
          ]
        },
        {
          "type": "formula",
          "as": "size",
          "expr": "datum.department_size"
        }
      ]
    },
    {
      "name": "data-major-movements",
      "source": "data-major",
      "transform": [
        {
          "type": "filter",
          "expr": "datum['__2__']!=datum['__3__']||datum['__2__']!=datum['__3__']"
        }
      ]
    },
    {
      "name": "data-pairs-by-month",
      "source": "data-major-movements",
      "transform": [
        {
          "type": "aggregate",
          "groupby": [
            "__0__",
            "loser",
            "gainer",
            "internal"
          ],
          "fields": [
            "count",
            "size"
          ],
          "ops": [
            "sum",
            "average"
          ],
          "as": [
            "count",
            "size"
          ]
        },
        {
          "type": "formula",
          "as": "movement",
          "expr": "datum.internal==1 ? '┇ '+datum.loser+' ┇' : datum.loser+' → '+datum.gainer"
        },
        {
          "type": "formula",
          "as": "churn_rate",
          "expr": "datum.count/datum.size"
        },
        {
          "type": "formula",
          "as": "value",
          "expr": "datum.count"
        }
      ]
    },
    {
      "name": "data-pairs",
      "source": "data-pairs-by-month",
      "transform": [
        {
          "type": "aggregate",
          "groupby": [
            "loser",
            "gainer",
            "internal",
            "movement"
          ],
          "fields": [
            "count",
            "churn_rate"
          ],
          "ops": [
            "sum",
            "average"
          ],
          "as": [
            "count",
            "churn_rate"
          ]
        },
        {
          "type": "formula",
          "as": "value",
          "expr": "datum.count"
        }
      ]
    },
    {
      "name": "data-pairs-by-month-sankey",
      "source": "data-aggregated",
      "transform": [
        {
          "type": "aggregate",
          "groupby": [
            "__0__",
            "loser",
            "gainer"
          ],
          "fields": [
            "count"
          ],
          "ops": [
            "sum"
          ],
          "as": [
            "count"
          ]
        },
        {
          "type": "formula",
          "as": "movement",
          "expr": "datum.loser+' → '+datum.gainer"
        }
      ]
    },
    {
      "name": "data-sankey",
      "source": "data-pairs-by-month-sankey",
      "transform": [
        {
          "type": "formula",
          "expr": "year(datum['__0__'])==2021?month(datum['__0__'])-10:month(datum['__0__'])+2",
          "as": "period"
        },
        {
          "type": "identifier",
          "as": "__1__"
        }
      ]
    },
    {
      "name": "data-sankey-losers",
      "source": "data-sankey",
      "transform": [
        {
          "type": "aggregate",
          "groupby": [
            "loser"
          ],
          "fields": [
            "loser"
          ],
          "ops": [
            "max"
          ],
          "as": [
            "unit"
          ]
        }
      ]
    },
    {
      "name": "data-sankey-gainers",
      "source": "data-sankey",
      "transform": [
        {
          "type": "aggregate",
          "groupby": [
            "gainer"
          ],
          "fields": [
            "gainer"
          ],
          "ops": [
            "max"
          ],
          "as": [
            "unit"
          ]
        }
      ]
    },
    {
      "name": "data-nodes-stack",
      "source": "data-sankey",
      "transform": [
        {
          "type": "fold",
          "fields": [
            "loser",
            "gainer"
          ],
          "as": [
            "key",
            "value"
          ]
        },
        {
          "type": "formula",
          "expr": "datum.key=='loser'?datum.loser+'-'+datum.gainer:datum.gainer+'-'+datum.loser",
          "as": "link"
        },
        {
          "type": "formula",
          "as": "order",
          "expr": "( datum.value=='Newcomer' || datum.value=='Left Company' ) ? 'Z' : split(datum.value,' ')[1]"
        },
        {
          "type": "formula",
          "as": "loser",
          "expr": "datum.period==5&&datum.key=='gainer'?datum.gainer:datum.loser"
        },
        {
          "type": "aggregate",
          "groupby": [
            "__0__",
            "loser",
            "period",
            "key",
            "value",
            "order"
          ],
          "fields": [
            "count"
          ],
          "ops": [
            "sum"
          ],
          "as": [
            "count"
          ]
        },
        {
          "type": "stack",
          "groupby": [
            "period",
            "key"
          ],
          "sort": {
            "field": "order",
            "order": "ascending"
          },
          "field": "count"
        },
        {
          "type": "filter",
          "expr": "datum.key=='loser'"
        },
        {
          "type": "aggregate",
          "groupby": [
            "__0__"
          ],
          "fields": [
            "count"
          ],
          "ops": [
            "sum"
          ],
          "as": [
            "count"
          ]
        }
      ]
    },
    {
      "name": "data-nodes",
      "source": "data-sankey",
      "transform": [
        {
          "type": "fold",
          "fields": [
            "loser",
            "gainer"
          ],
          "as": [
            "key",
            "value"
          ]
        },
        {
          "type": "formula",
          "expr": "datum.key=='loser'?datum.loser+'-'+datum.gainer:datum.gainer+'-'+datum.loser",
          "as": "link"
        },
        {
          "type": "formula",
          "as": "order",
          "expr": "( datum.value=='Newcomer' || datum.value=='Left Company' ) ? 'Z' : split(datum.value,' ')[1]"
        },
        {
          "type": "stack",
          "groupby": [
            "period",
            "key"
          ],
          "sort": {
            "field": "order",
            "order": "ascending"
          },
          "field": "count"
        },
        {
          "type": "lookup",
          "from": "data-nodes-stack",
          "key": "__0__",
          "fields": [
            "__0__"
          ],
          "values": [
            "count"
          ],
          "as": [
            "stack"
          ]
        },
        {
          "type": "formula",
          "expr": "20+(datum.value=='Newcomer'?(datum.y0-datum.stack-sankeyShift):datum.value=='Left Company'?(datum.y0+sankeyShift):datum.y0)",
          "as": "y0"
        },
        {
          "type": "formula",
          "expr": "20+(datum.value=='Newcomer'?(datum.y1-datum.stack-sankeyShift):datum.value=='Left Company'?(datum.y1+sankeyShift):datum.y1)",
          "as": "y1"
        },
        {
          "type": "formula",
          "expr": "(datum.y0+datum.y1)/2",
          "as": "yc"
        }
      ]
    },
    {
      "name": "data-nodes-grouped",
      "source": "data-sankey",
      "transform": [
        {
          "type": "fold",
          "fields": [
            "loser",
            "gainer"
          ],
          "as": [
            "key",
            "value"
          ]
        },
        {
          "type": "formula",
          "as": "order",
          "expr": "( datum.value=='Newcomer' || datum.value=='Left Company' ) ? 'Z' : split(datum.value,' ')[1]"
        },
        {
          "type": "formula",
          "as": "loser",
          "expr": "datum.value=='Newcomer'?datum.loser:datum.value=='Left Company'?datum.gainer:(datum.period==5&&datum.key=='gainer')?datum.gainer:datum.loser"
        },
        {
          "type": "aggregate",
          "groupby": [
            "__0__",
            "loser",
            "period",
            "key",
            "value",
            "order"
          ],
          "fields": [
            "count"
          ],
          "ops": [
            "sum"
          ],
          "as": [
            "count"
          ]
        },
        {
          "type": "stack",
          "groupby": [
            "period",
            "key"
          ],
          "sort": {
            "field": "order",
            "order": "ascending"
          },
          "field": "count"
        },
        {
          "type": "extent",
          "field": "y1",
          "signal": "extSank"
        },
        {
          "type": "lookup",
          "from": "data-nodes-stack",
          "key": "__0__",
          "fields": [
            "__0__"
          ],
          "values": [
            "count"
          ],
          "as": [
            "stack"
          ]
        },
        {
          "type": "formula",
          "expr": "20+(datum.value=='Newcomer'?(datum.y0-datum.stack-sankeyShift):datum.value=='Left Company'?(datum.y0+sankeyShift):datum.y0)",
          "as": "y0"
        },
        {
          "type": "formula",
          "expr": "20+(datum.value=='Newcomer'?(datum.y1-datum.stack-sankeyShift):datum.value=='Left Company'?(datum.y1+sankeyShift):datum.y1)",
          "as": "y1"
        },
        {
          "type": "formula",
          "expr": "(datum.y0+datum.y1)/2",
          "as": "yc"
        }
      ]
    },
    {
      "name": "data-nodes-gainer",
      "source": "data-nodes",
      "transform": [
        {
          "type": "filter",
          "expr": "datum.key == 'gainer'"
        }
      ]
    },
    {
      "name": "data-links",
      "source": "data-nodes",
      "transform": [
        {
          "type": "filter",
          "expr": "datum.key == 'loser'"
        },
        {
          "type": "lookup",
          "from": "data-nodes-gainer",
          "key": "__1__",
          "fields": [
            "__1__"
          ],
          "as": [
            "gainer"
          ]
        },
        {
          "type": "linkpath",
          "orient": "horizontal",
          "shape": "diagonal",
          "sourceX": {
            "expr": "30+20+datum.period*sankeyMonthWidth+(datum.key=='loser'?20:0)"
          },
          "sourceY": {
            "expr": "scale('scaleYSankey', datum.yc)"
          },
          "targetX": {
            "expr": "30+(datum.period+1)*sankeyMonthWidth"
          },
          "targetY": {
            "expr": "scale('scaleYSankey', datum.gainer.yc)"
          }
        },
        {
          "type": "formula",
          "expr": "max(1,sankeyGroupHeight-scale('scaleYSankey', datum.count))",
          "as": "strokeWidth"
        },
        {
          "type": "filter",
          "expr": "datum.loser!==datum.gainer.gainer"
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "scaleXSankey",
      "type": "time",
      "domain": {
        "data": "data-aggregated",
        "field": "__0__",
        "sort": true
      },
      "range": [
        {
          "signal": "sankeyMonthWidth/2+sankeyMonthWidth*0.25+12"
        },
        {
          "signal": "sankeyGroupWidth-sankeyMonthWidth/2-sankeyMonthWidth*0.25-8"
        }
      ]
    },
    {
      "name": "scaleYSankey",
      "type": "linear",
      "domain": {
        "data": "data-nodes",
        "field": "y1"
      },
      "range": [
        25,
        {
          "signal": "sankeyGroupHeight"
        }
      ],
      "reverse": true
    },
    {
      "name": "scaleOpacity",
      "type": "pow",
      "exponent": 2,
      "domain": {
        "data": "data-pairs",
        "field": "value"
      },
      "range": [
        0.45,
        1
      ]
    }
  ],
  "title": {
    "text": {
      "signal": "'Transfers Between Departments'"
    },
    "anchor": "start",
    "align": "left",
    "frame": "group",
    "dx": -10,
    "dy": 30,
    "subtitle": {
      "signal": "'Excluding transfers within the same department'"
    }
  },
  "axes": [
    {
      "scale": "scaleXSankey",
      "orient": "bottom",
      "domain": false,
      "offset": 0,
      "labelFontSize": 14,
      "encode": {
        "labels": {
          "update": {
            "text": {
              "signal": "year(datum.value)+'-'+truncate(('0'+(month(datum.value)+1)),2,'left','')"
            }
          }
        }
      }
    }
  ],
  "marks": [
    {
      "name": "rect-nodes",
      "type": "rect",
      "from": {
        "data": "data-nodes-grouped"
      },
      "encode": {
        "update": {
          "x": {
            "signal": "30+((datum.key=='gainer'?1:0)+datum.period)*sankeyMonthWidth"
          },
          "y": {
            "signal": "scale('scaleYSankey',datum.y0)"
          },
          "y2": {
            "signal": "(scale('scaleYSankey',datum.y0)-scale('scaleYSankey',datum.y1)<1?1:0)+scale('scaleYSankey',datum.y1)"
          },
          "stroke": {
            "value": "white"
          },
          "strokeWidth": {
            "value": 1.5
          },
          "opacity": {
            "signal": "sankeyOpacity"
          },
          "fill": {
            "signal": "datum.key=='gainer'?(datum.value=='Left Company'?colorLeft:stripe): datum.key=='loser'?(datum.value=='Newcomer'?colorNew:stripe): stripe"
          },
          "width": {
            "signal": "datum.order=='Z' ? 40 :(datum.period==5 ? 40 : datum.period!=5&&datum.key=='loser' ? 40 : 0)"
          },
          "tooltip": {
            "signal": "datum.value+': '+datum.count"
          }
        }
      }
    },
    {
      "name": "rule-axis-x",
      "type": "rule",
      "zindex": 20,
      "encode": {
        "update": {
          "x": {
            "signal": "0"
          },
          "x2": {
            "signal": "sankeyGroupWidth"
          },
          "y": {
            "value": "0",
            "scale": "scaleYSankey",
            "offset": -45
          },
          "y2": {
            "value": "0",
            "scale": "scaleYSankey",
            "offset": -45
          },
          "strokeWidth": {
            "signal": "0.5"
          },
          "stroke": {
            "signal": "'grey'"
          }
        }
      }
    },
    {
      "type": "path",
      "name": "path-links",
      "from": {
        "data": "data-links"
      },
      "encode": {
        "update": {
          "path": {
            "field": "path"
          },
          "stroke": [
            {
              "test": "datum.gainer.gainer=='Left Company'",
              "signal": "colorLeft"
            },
            {
              "test": "datum.loser=='Newcomer'",
              "signal": "colorNew"
            },
            {
              "signal": "colorNeutral"
            }
          ],
          "strokeWidth": {
            "field": "strokeWidth",
            "offset": -1.5
          },
          "strokeOpacity": {
            "signal": "sankeyOpacity"
          },
          "tooltip": {
            "signal": "datum.movement+': '+datum.count"
          },
          "cursor": {
            "value": "pointer"
          }
        }
      }
    },
    {
      "type": "text",
      "from": {
        "data": "data-nodes-grouped"
      },
      "encode": {
        "update": {
          "text": {
            "signal": "(scale('scaleYSankey', (datum.y1-datum.y0))>=10&&datum.period==0&&datum.key=='loser')?datum.loser!='Newcomer'?datum.loser+'-':'':''"
          },
          "x": {
            "signal": "30"
          },
          "y": {
            "signal": "(datum.y0+datum.y1)/2",
            "scale": "scaleYSankey"
          },
          "fill": {
            "signal": "colorLabels"
          },
          "fontSize": {
            "signal": "12"
          },
          "align": {
            "value": "right"
          },
          "baseline": {
            "value": "middle"
          }
        }
      }
    },
    {
      "type": "text",
      "from": {
        "data": "data-nodes-grouped"
      },
      "encode": {
        "update": {
          "text": {
            "signal": "(scale('scaleYSankey', (datum.y1-datum.y0))>=10&&datum.period==5&&datum.key=='loser')?datum.loser!='Left Company'?'-'+datum.loser:'':''"
          },
          "x": {
            "signal": "sankeyGroupWidth-30+5"
          },
          "y": {
            "signal": "(datum.y0+datum.y1)/2",
            "scale": "scaleYSankey"
          },
          "fill": {
            "signal": "colorNeutral"
          },
          "fontSize": {
            "signal": "12"
          },
          "align": {
            "value": "left"
          },
          "baseline": {
            "value": "middle"
          }
        }
      }
    }
  ]
}
