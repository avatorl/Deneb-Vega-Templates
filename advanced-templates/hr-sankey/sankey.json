        }
      ]
    },
    {
      "name": "data-structure-d-previous",
      "source": "data-structure-base-previous",
      "transform": [
        {
          "type": "aggregate",
          "groupby": [
            "__2__"
          ],
          "fields": [
            "count"
          ],
          "ops": [
            "sum"
          ],
          "as": [
            "count"
          ]
        },
        {
          "type": "stack",
          "field": "count",
          "groupby": [
            "type"
          ],
          "sort": {
            "field": [
