  "data": [
    {
      "name": "dataset"
    },
    {
      "name": "data-aggregated",
      "source": "dataset",
      "transform": [
        {
          "type": "aggregate",
          "groupby": [
            "__0__",
            "__2__",
            "__2__",
            "__3__",
            "__3__"
          ]
        },
        {
          "type": "formula",
          "as": "loser",
